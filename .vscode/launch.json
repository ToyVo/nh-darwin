    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug nh search",
            "cargo": {
                "args": ["build", "--bin=nh-darwin", "--package=nh-darwin"],
                "filter": {
                    "name": "nh-darwin",
                    "kind": "bin"
                }
            },
            "args": [
                "search",
                "vscode"
            ],
            "cwd": "${workspaceFolder}"
        },
    ]
}
